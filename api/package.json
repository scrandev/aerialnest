{
  "name": "aerialnest-api",
  "version": "1.0.0",
  "description": "Aerial Nest backend API",
  "main": "src/index.js",
  "scripts": {
    "dev": "nodemon src/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "build": "zip -r lambda-code.zip src/ -x '*.test.js'",
    "deploy": "npm run build && aws s3 cp lambda-code.zip s3://aerialnest-deployment-code/api/",
    "deploy:layer": "mkdir -p layer/nodejs && cp -r node_modules layer/nodejs/ && cd layer && zip -r ../lambda-layer-node-modules.zip . && cd .. && aws s3 cp lambda-layer-node-modules.zip s3://aerialnest-deployment-code/api/"
  },
  "dependencies": {
    "pg": "^8.11.3",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "express": "^4.18.2",
    "multer": "^1.4.5-lts.1",
    "aws-sdk": "^2.1478.0"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "nodemon": "^3.0.1",
    "supertest": "^6.3.3"
  },
  "keywords": ["end-of-life", "document-management", "legacy", "planning"],
  "author": "Aerial Nest Team",
  "license": "UNLICENSED"
}
EOF
